<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="findologic_d_i.view_dir">%findologic_d_i.plugin_dir%/Resources/views</parameter>
    </parameters>

    <services>
        <service id="findologic_d_i.subscriber.frontend" class="findologicDI\Subscriber\Frontend">
            <tag name="shopware.event_subscriber" />
        </service>

        <service id="findologic_d_i.shopware_process" class="findologicDI\ShopwareProcess">

        </service>


        <service id="findologic_d_i.commands.export_command"
                 class="findologicDI\Commands\ExportCommand">
            <tag name="console.command"/>
        </service>

        <service id="shopware.api.findologic" class="findologicDI\Components\Api\Resource\Findologic">

        </service>
    </services>
</container>